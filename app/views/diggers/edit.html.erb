<div class="container">
  <%= simple_form_for @digger do |f| %>
  <%= f.input :name %>
  <%= f.input :technical_specification %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :day_rate %>
  <%= f.input :photo %>
   <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Dig it!", class: "btn btn-secondary" %>
<% end %>
</div>

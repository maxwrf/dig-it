<div class="container p-4  ">
  <%= simple_form_for(@user, url: dashboard_edit_path)  do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :email %>
  <%= f.input :photo %>
   <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Update", class: "btn btn-secondary" %>
  <a class="btn btn-secondary" href="/dashboard" role="button">Back</a>
<% end %>
</div>
